<?xml version="1.0" encoding="utf-8"?>

<selector
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/com_size_0010dp"/>
            <solid android:color="@color/btn_pressed"/>
            <stroke android:width="@dimen/com_size_0002dp"
                android:color="@color/btn_pressed"/>
            <size android:width="@dimen/com_size_0480dp" android:height="@dimen/com_size_0068dp"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/com_size_0010dp"/>
            <solid android:color="@color/btn_pressed"/>
            <stroke android:width="@dimen/com_size_0002dp"
                android:color="@color/btn_pressed"/>
            <size android:width="@dimen/com_size_0480dp" android:height="@dimen/com_size_0068dp"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/com_size_0010dp"/>
            <stroke android:width="@dimen/com_size_0002dp"
                android:color="@color/login_btn_color"/>
            <size android:width="@dimen/com_size_0480dp" android:height="@dimen/com_size_0068dp"/>
        </shape>
    </item>
</selector>